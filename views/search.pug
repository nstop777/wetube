extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos found
        each item in videos
            +videoBlock({
                id: item.id,
                videoFile: item.videoFile,
                title: item.title,
                views: item.views
            })

            